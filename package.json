{
	"name": "besidka",
	"type": "module",
	"packageManager": "pnpm@10.30.1",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "ROLLDOWN_OPTIONS_VALIDATION=loose nuxt build",
		"dev": "nuxt dev --dotenv .dev.vars",
		"preview": "pnpm run build && wrangler dev --port 3000",
		"remote:preview": "pnpm run build && wrangler dev --port 3000 --remote",
		"prod:preview": "pnpm run build && wrangler dev --port 3000 --env production --remote",
		"deploy": "pnpm run build && wrangler deploy",
		"prod:deploy": "pnpm run build && wrangler deploy --env production",
		"generate": "nuxt generate",
		"dev:prepare": "nuxt prepare",
		"typecheck": "nuxt typecheck",
		"analyze": "ROLLDOWN_OPTIONS_VALIDATION=loose nuxt analyze",
		"cf-typegen": "wrangler types",
		"lint": "eslint",
		"format": "eslint --fix",
		"test": "vitest",
		"test:unit": "vitest run tests/unit",
		"test:unit:changed": "vitest run tests/unit --changed",
		"test:unit:related": "vitest run tests/unit --related",
		"test:unit:watch": "vitest tests/unit",
		"test:integration": "vitest run tests/integration --passWithNoTests",
		"test:integration:watch": "vitest tests/integration",
		"test:ui": "vitest --ui",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui --headed --debug",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:chromium": "playwright test --project=chromium",
		"test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
		"test:affected": "node scripts/test-affected.mjs",
		"test:affected:unit": "node scripts/test-affected.mjs --type=unit",
		"test:affected:integration": "node scripts/test-affected.mjs --type=integration",
		"test:affected:e2e": "node scripts/test-affected.mjs --type=e2e",
		"test:affected:fast": "node scripts/test-affected.mjs --type=unit && node scripts/test-affected.mjs --type=integration",
		"coverage": "vitest run --coverage --passWithNoTests",
		"coverage:ui": "vitest --coverage --ui",
		"db:generate": "drizzle-kit generate",
		"db:reset": "rm -rf .wrangler/state/v3/d1 && drizzle-kit generate && pnpm exec wrangler d1 migrations apply DB",
		"db:migrate": "pnpm exec wrangler d1 migrations apply DB",
		"remote:db:migrate": "pnpm exec wrangler d1 migrations apply DB --remote",
		"prod:db:migrate": "pnpm exec wrangler d1 migrations apply DB --env production --remote",
		"db:studio": "drizzle-kit studio",
		"prepare": "husky",
		"postinstall": "node scripts/setup-ai-agents.mjs && nuxt prepare"
	},
	"dependencies": {
		"@ai-sdk/google": "^3.0.30",
		"@ai-sdk/openai": "^3.0.26",
		"@ai-sdk/provider": "^3.0.8",
		"@ai-sdk/vue": "^3.0.97",
		"@iconify-json/lucide": "^1.2.93",
		"@nuxt/eslint": "^1.15.1",
		"@nuxt/fonts": "^0.14.0",
		"@nuxt/icon": "^2.2.1",
		"@nuxt/image": "^2.0.0",
		"@nuxtjs/color-mode": "^3.5.2",
		"@nuxtjs/device": "^3.2.4",
		"@nuxtjs/mdc": "^0.20.1",
		"@tailwindcss/vite": "^4.2.1",
		"@vite-pwa/nuxt": "^1.1.1",
		"@vueuse/core": "^14.2.1",
		"@vueuse/nuxt": "^14.2.1",
		"ai": "^6.0.97",
		"better-auth": "^1.4.19",
		"crypto-shield": "^1.6.2",
		"daisyui": "^5.5.19",
		"defu": "^6.1.4",
		"drizzle-kit": "^0.31.9",
		"drizzle-orm": "0.45.1",
		"drizzle-query-logger": "^1.1.2",
		"eslint": "^9.39.2",
		"evlog": "1.11.0",
		"hashids": "^2.3.0",
		"nuxt": "^4.3.1",
		"nuxt-svgo": "^4.2.6",
		"remark-breaks": "^4.0.0",
		"resend": "6.8.0",
		"sanitize-html": "^2.17.1",
		"shiki": "^3.22.0",
		"shiki-stream": "^0.1.4",
		"tailwindcss": "^4.2.1",
		"ulid": "^3.0.2",
		"vue": "^3.5.28",
		"vue-router": "^5.0.3",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20260302.0",
		"@commitlint/cli": "^20.4.2",
		"@commitlint/config-conventional": "^20.4.2",
		"@commitlint/format": "^20.4.0",
		"@commitlint/types": "^20.4.0",
		"@nuxt/test-utils": "^4.0.0",
		"@playwright/test": "^1.58.2",
		"@stylistic/eslint-plugin": "^5.9.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^24.10.12",
		"@types/sanitize-html": "^2.16.0",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"@vue/test-utils": "^2.4.6",
		"better-sqlite3": "^12.6.2",
		"happy-dom": "^20.7.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"msw": "^2.12.9",
		"playwright-core": "^1.58.2",
		"typescript": "^5.9.3",
		"vite-plugin-eslint2": "^5.0.5",
		"vitest": "^4.0.18",
		"vitest-environment-nuxt": "^1.0.1",
		"vue-tsc": "^3.2.5",
		"whatwg-fetch": "^3.6.20",
		"wrangler": "^4.67.0"
	},
	"peerDependencies": {
		"@react-email/render": "^2.0.4"
	},
	"peerDependenciesMeta": {
		"@react-email/render": {
			"optional": true
		}
	},
	"onlyBuiltDependencies": [
		"rolldown",
		"@parcel/watcher",
		"@tailwindcss/oxide",
		"better-sqlite3",
		"esbuild",
		"sharp",
		"unrs-resolver",
		"workerd"
	],
	"overrides": {
		"vite": "npm:rolldown-vite@latest"
	},
	"lint-staged": {
		"*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}": [
			"eslint --fix"
		]
	}
}