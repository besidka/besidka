{
	"name": "besidka",
	"type": "module",
	"packageManager": "pnpm@10.17.1",
	"engines": {
		"node": ">=24",
		"npm": ">=11.6"
	},
	"scripts": {
		"build": "ROLLDOWN_OPTIONS_VALIDATION=loose nuxt build",
		"dev": "nuxt dev --dotenv .dev.vars",
		"preview": "pnpm run build && wrangler dev --port 3000",
		"upload": "pnpm run build && wrangler versions upload",
		"deploy": "pnpm run build && wrangler deploy",
		"generate": "nuxt generate",
		"dev:prepare": "nuxt prepare",
		"typecheck": "nuxt typecheck",
		"analyze": "ROLLDOWN_OPTIONS_VALIDATION=loose nuxt analyze",
		"postinstall": "nuxt prepare",
		"cf-typegen": "wrangler types",
		"lint": "eslint",
		"format": "eslint --fix",
		"test": "vitest",
		"coverage": "vitest run --coverage --passWithNoTests",
		"db:generate": "drizzle-kit generate",
		"db:reset": "rm -rf .wrangler/state/v3/d1 && drizzle-kit generate && pnpx wrangler d1 migrations apply DB",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"prepare": "husky"
	},
	"dependencies": {
		"@ai-sdk/google": "^2.0.29",
		"@ai-sdk/openai": "^2.0.64",
		"@ai-sdk/provider": "^2.0.0",
		"@ai-sdk/vue": "^2.0.89",
		"@iconify-json/lucide": "^1.2.72",
		"@nuxt/eslint": "1.10.0",
		"@nuxt/fonts": "0.12.1",
		"@nuxt/icon": "2.1.0",
		"@nuxt/image": "^2.0.0",
		"@nuxtjs/color-mode": "^3.5.2",
		"@nuxtjs/device": "^4.0.0",
		"@nuxtjs/mdc": "^0.18.2",
		"@tailwindcss/vite": "^4.1.17",
		"@vite-pwa/nuxt": "^1.0.7",
		"@vueuse/core": "^14.0.0",
		"@vueuse/nuxt": "^14.0.0",
		"ai": "^5.0.89",
		"better-auth": "1.3.34",
		"crypto-shield": "^1.6.2",
		"daisyui": "5.4.7",
		"defu": "^6.1.4",
		"drizzle-kit": "^0.31.6",
		"drizzle-orm": "^0.44.7",
		"drizzle-query-logger": "^1.1.1",
		"eslint": "^9.39.1",
		"hashids": "^2.3.0",
		"nuxt": "^4.2.1",
		"nuxt-svgo": "^4.2.6",
		"remark-breaks": "^4.0.0",
		"resend": "^6.4.2",
		"sanitize-html": "^2.17.0",
		"shiki": "^3.15.0",
		"shiki-stream": "^0.1.3",
		"tailwindcss": "4.1.17",
		"ulid": "^3.0.1",
		"vue": "^3.5.24",
		"vue-router": "^4.6.3",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20260130.0",
		"@commitlint/cli": "^20.4.0",
		"@commitlint/config-conventional": "^20.4.0",
		"@commitlint/format": "^20.4.0",
		"@commitlint/types": "^20.4.0",
		"@nuxt/test-utils": "^3.23.0",
		"@stylistic/eslint-plugin": "^5.7.1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^24.10.0",
		"@types/sanitize-html": "^2.16.0",
		"@vitest/coverage-v8": "^4.0.18",
		"@vue/test-utils": "^2.4.6",
		"better-sqlite3": "^12.6.2",
		"conventional-changelog-atom": "^5.0.0",
		"happy-dom": "^20.4.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"nitro-cloudflare-dev": "^0.2.2",
		"nitropack": "^2.13.1",
		"typescript": "^5.9.3",
		"vite-plugin-eslint2": "^5.0.4",
		"vitest": "^4.0.18",
		"vue-tsc": "^3.2.4",
		"wrangler": "^4.61.1"
	},
	"onlyBuiltDependencies": [
		"rolldown",
		"@parcel/watcher",
		"@tailwindcss/oxide",
		"better-sqlite3",
		"esbuild",
		"sharp",
		"unrs-resolver",
		"workerd"
	],
	"overrides": {
		"vite": "npm:rolldown-vite@latest"
	},
	"lint-staged": {
		"*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}": [
			"eslint --fix",
			"vitest related --run --passWithNoTests"
		]
	}
}